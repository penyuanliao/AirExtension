<?xml version="1.0" encoding="UTF-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/17.0">

    <id>com.ane.os.device.info</id>
    <versionNumber>1.0.1</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libairExtension.a</nativeLibrary>
                <initializer>AirExtInitializer</initializer>
                <finalizer>AirExtFinalizer</finalizer>
            </applicationDeployment>
        </platform>
        
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>libairJavaExtension.jar</nativeLibrary>
                <initializer>com.ane.android.airExtension</initializer>
            </applicationDeployment>
        </platform>
        
        <platform name="default">
            <applicationDeployment/>
        </platform>
        
    </platforms>
</extension>